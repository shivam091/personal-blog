{
  "name": "personal-blog",
  "version": "1.0.0",
  "description": "Harshal LADHE's personal blog",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/shivam091/personal-blog.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shivam091/personal-blog/issues"
  },
  "homepage": "https://harshal-ladhe.netlify.app",
  "scripts": {
    "build": "node esbuild.config.js",
    "watch": "node esbuild.config.js --watch",
    "clean": "bundle exec jekyll clean && rm -rf dist",
    "jekyll": "bundle exec jekyll serve",
    "start": "npm run build && concurrently \"npm run watch\" \"npm run jekyll\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "counterapi": "^2.1.2",
    "esbuild": "^0.25.5",
    "esbuild-sass-plugin": "^3.3.1",
    "prettier": "3.6.2",
    "recaptcha-v3": "^1.11.3",
    "typed.js": "^2.0.16",
    "vanilla-lazyload": "^19.1.3"
  }
}
